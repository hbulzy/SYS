<!DOCTYPE>
<html>
@using (Html.BeginHead())
{ 
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>升级程序</title>
    <link href="@Url.Content("~/Applications/Setup/Styles/style.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery/jquery-1.7.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/validate/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/validate/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/tunynet/jquery.validate.additional-methods.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/tunynet/jquery.validate.messages-zh-CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/ajaxForm/jquery.blockUI.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/ajaxForm/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/sliderkit/jquery.sliderkit-1.9.1.js")" type="text/javascript"></script>
}
<body>
    <div class="spb-under spb-upgrade">
        <div class="spb-fix">
            <div class="spb-hd">
                <div class="spb-bg spb-logo">
                    <h1>近乎</h1>
                    <span class="spb-upgrade-title">升级程序</span>
                </div>
                <em>v4.2</em>
            </div>
            @RenderBody()
            @RenderSection("Footer", false)
        </div>
    </div>
    @Scripts.Render("~/Bundle/Scripts/Site")
    @RenderSection("Scripts", false)
    @Html.RenderScripts()
    @Html.RenderScriptBlocks()
</body>
</html>
